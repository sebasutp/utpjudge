<%- model_class = Submission -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:init_date) %></th>
      <th><%= model_class.human_attribute_name(:end_date) %></th>
      <th><%= model_class.human_attribute_name(:veredict) %></th>
      <th><%= model_class.human_attribute_name(:time) %></th>
      <th><%= model_class.human_attribute_name(:exercise_problem_id) %></th>
      <th><%= model_class.human_attribute_name(:srcfile_file_name) %></th>
      <th><%= model_class.human_attribute_name(:srcfile_content_type) %></th>
      <th><%= model_class.human_attribute_name(:srcfile_file_size) %></th>
      <th><%= model_class.human_attribute_name(:srcfile_updated_at) %></th>
      <th><%= model_class.human_attribute_name(:outfile_file_name) %></th>
      <th><%= model_class.human_attribute_name(:outfile_content_type) %></th>
      <th><%= model_class.human_attribute_name(:outfile_file_size) %></th>
      <th><%= model_class.human_attribute_name(:outfile_updated_at) %></th>
      <th><%= model_class.human_attribute_name(:tcaseId) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= link_to submission.id, submission_path(submission) %></td>
        <td><%= submission.init_date %></td>
        <td><%= submission.end_date %></td>
        <td><%= submission.veredict %></td>
        <td><%= submission.time %></td>
        <td><%= submission.exercise_problem_id %></td>
        <td><%= submission.srcfile_file_name %></td>
        <td><%= submission.srcfile_content_type %></td>
        <td><%= submission.srcfile_file_size %></td>
        <td><%= submission.srcfile_updated_at %></td>
        <td><%= submission.outfile_file_name %></td>
        <td><%= submission.outfile_content_type %></td>
        <td><%= submission.outfile_file_size %></td>
        <td><%= submission.outfile_updated_at %></td>
        <td><%= submission.tcaseId %></td>
        <td><%=l submission.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_submission_path(submission), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      submission_path(submission),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_submission_path,
            :class => 'btn btn-primary' %>
